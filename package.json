  "scripts": {
    "docs": "apidoc -i app/ -o apidoc/",
    "start": "node app.js",
    "start:db": "sudo /usr/local/mysql/support-files/mysql.server start",
    "start:local": "ST_VA_APP_KEY=test ST_VA_API_KEY=test ST_ENV=local LISTLY_DB_HOST_NAME=test LISTLY_DB_USER=test LISTLY_DB_PORT=test LISTLY_DB_PASSWORD=test LISTLY_DB_DATABASE=test LISTLY_MANDRILL_SECRET_KEY=test node app.js",
    "start:dev": "ST_ENV=dev LISTLY_DB_HOST_NAME=test LISTLY_DB_USER=test LISTLY_DB_PORT=test LISTLY_DB_PASSWORD=test LISTLY_DB_DATABASE=test LISTLY_MANDRILL_SECRET_KEY=test node app.js",
    "start:production": "ST_ENV=production LISTLY_DB_HOST_NAME=test LISTLY_DB_USER=test LISTLY_DB_PORT=test LISTLY_DB_PASSWORD=test LISTLY_DB_DATABASE=test LISTLY_MANDRILL_SECRET_KEY=test node app.js",
    "devdb": "mongod --dbpath ~/data/st-mongodb",
    "test": "ST_ENV=local jasmine"
  },
