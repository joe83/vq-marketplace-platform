{
  "name": "vq-marketplace-api",
  "version": "3.0.1",
  "description": "VQ Web Services",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vq-labs/vq-marketplace-api"
  },
  "scripts": {
    "lint": "node ./node_modules/.bin/eslint src",
    "tslint": "node ./node_modules/tslint/bin/tslint -c tslint.json -p tsconfig.json",
    "build:nolint": "node ./node_modules/gulp/bin/gulp.js build",
    "build": "npm run tslint && node ./node_modules/gulp/bin/gulp.js build",
    "start": "node --inspect=5858 ./node_modules/ts-node/dist/bin.js src/app.js",
    "dev": "node ./node_modules/nodemon/bin/nodemon.js",
    "create-marketplace": "node ./scripts/create-marketplace.js",
    "test": "node ./node_modules/jasmine-ts/lib/index.js \"spec/**/*[sS]pec.js\""
  },
  "author": "vq-labs.com",
  "license": "MIT",
  "dependencies": {
    "address-validator": "^0.2.2",
    "async": "^2.0.0-rc.6",
    "aws-sdk": "^2.7.14",
