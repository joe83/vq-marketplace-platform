    "build": "npm run tslint && rm -rf ./built && gulp build",
    "start:click4work": "npm run build && TENANT_ID=click4work node ./built/app --config /../../../production-config.json",
    "start": "npm run build && node built/app.js",
    "start:local": "npm run build && node built/app.js",
    "start:dev": "npm run build && node built/app --config /../../test-config.json",
    "test": "ST_ENV=local jasmine"
  },
