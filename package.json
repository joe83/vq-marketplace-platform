    "@types/stripe": "^4.8.0",
    "bson": "^1.0.4",
    "eslint": "^4.9.0",
    "gulp": "^3.9.1",
    "gulp-typescript": "^3.2.3",
    "husky": "^0.15.0-rc.8",
    "jasmine-node": "^1.14.5",
    "jasmine-ts": "^0.2.1",
    "mongodb": "^2.2.33",
    "nodemon": "^1.15.1",
    "pre-commit": "^1.2.2",
