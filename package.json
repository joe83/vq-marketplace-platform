{
  "name": "vq-marketplace-api",
  "version": "3.0.1",
  "description": "VQ Web Services",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vq-labs/vq-marketplace-api"
  },
  "scripts": {
    "start": "node build/app.js",
    "create:vqbackend": "node ./build/cli/create-vqbackend.js blank vqbackend en",
    "start:vqbackend": "TENANT_ID=vqbackend npm run start",
    "lint": "node ./node_modules/.bin/eslint src",
    "tslint": "node ./node_modules/tslint/bin/tslint -c tslint.json -p tsconfig.json",
    "build:nolint": "rm -rf ./build && node ./node_modules/gulp/bin/gulp.js build",
    "build": "rm -rf ./build && npm run tslint && node ./node_modules/gulp/bin/gulp.js build",
    "start:debug": "node --inspect=5858 ./node_modules/ts-node/dist/bin.js src/app.js",
    "dev": "node ./node_modules/nodemon/bin/nodemon.js",
    "create-marketplace": "node ./scripts/create-marketplace.js",
    "jasmine": "node ./node_modules/jasmine-ts/lib/index.js \"spec/**/*[sS]pec.js\"",
    "test": "npm run test:signup && npm run test:tenant",
    "test:signup": "node ./node_modules/jasmine-ts/lib/index.js ./spec/signupsSpec.js",
    "test:tenant": "node ./node_modules/jasmine-ts/lib/index.js ./spec/tenantSpec.js"
  },
  "author": "vq-labs.com",
  "license": "MIT",
  "dependencies": {
    "address-validator": "^0.2.2",
    "async": "^2.0.0-rc.6",
    "aws-sdk": "^2.7.14",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.15.2",
    "chargebee": "^2.3.4",
    "cookie-parser": "~1.4.3",
    "cors": "^2.7.2",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^4.0.0",
    "ejs": "~2.5.5",
    "express": "^4.15.2",
    "fs": "0.0.2",
    "fs-extra": "^5.0.0",
    "https": "~1.0.0",
    "iban": "0.0.7",
    "install": "^0.8.2",
    "jsonfile": "~2.4.0",
    "jsonwebtoken": "~7.2.1",
    "lodash": "^4.17.2",
    "mandrill-api": "~1.0.45",
    "minimist": "^1.2.0",
    "moment": "~2.17.1",
    "moment-timezone": "~0.5.10",
    "morgan": "~1.7.0",
    "multer": "~1.2.1",
    "mysql2": "^1.6.1",
    "node-geocoder": "^3.20.0",
    "node-slack": "0.0.7",
    "npm": "^4.0.3",
    "passport": "~0.3.2",
    "passport-facebook": "~2.1.1",
    "passport-local": "~1.0.0",
    "ping": "^0.2.1",
    "rand-token": "~0.3.0",
    "random-token": "0.0.8",
    "randomstring": "^1.1.5",
    "request": "~2.79.0",
    "sequelize": "^4.39.0",
    "sharp": "^0.17.2",
    "stripe": "^4.25.0",
    "striptags": "^3.1.0",
    "underscore": "^1.8.3",
    "unescape": "^1.0.1",
    "utf8": "~2.1.1",
    "when": "~3.7.7"
  },
  "devDependencies": {
    "@types/async": "^2.0.45",
    "@types/express": "^4.11.1",
    "@types/randomstring": "^1.1.6",
    "@types/stripe": "^4.8.0",
    "bson": "^1.0.4",
    "eslint": "^4.9.0",
    "gulp": "^3.9.1",
    "gulp-apidoc": "^0.2.8",
    "gulp-typescript": "^3.2.3",
    "jasmine-node": "^1.14.5",
    "jasmine-ts": "^0.2.1",
    "mongodb": "^2.2.33",
    "nodemon": "^1.15.1",
    "pre-commit": "^1.2.2",
    "request-json": "^0.6.2",
    "ts-node": "^5.0.0",
    "tslint": "^5.8.0",
    "typescript": "^2.7.2",
